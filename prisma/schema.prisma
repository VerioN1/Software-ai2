generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Role {
  id   Int    @id @default(autoincrement())
  name String @unique
  User User[]
}

model User {
  id             String  @id @default(uuid())
  firstName      String  @db.NVarChar(50)
  lastName       String  @db.NVarChar(50)
  password       String
  email          String  @unique @db.VarChar(200)
  emailConfirmed Boolean @default(false)
  tokenEmail     String? @db.VarChar(32)
  remark         String? @db.NVarChar(100) // what is this?
  userRole       Role    @relation(fields: [roleId], references: [id])
  roleId         Int
  rowAmount      Int? // what is this?
}

model Alerts {
