  CustomerNumber    String?  @db.NVarChar(20)
  AlertName         String?  @db.NVarChar(50)
  AlertMessage      String?  @db.NVarChar(1024)
  Sender            String?  @db.NVarChar(1024)
  ParamID1          Int?
  ParamID2          Int?
  ParamID3          Int?
  Email1            String?  @db.NVarChar(1024)
  Email2            String?  @db.NVarChar(1024)
